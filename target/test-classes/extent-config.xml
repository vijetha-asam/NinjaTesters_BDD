<!-- global date format override -->  <!-- defaults to HH:MM:SS -->
<timeformate> HH:MM:SS</timeformate>


<!-- location of charts in the test view -->  <!-- top,bottom -->
<testviewChartLocstion>bottom</testviewChartLocstion>

<!-- custom javascript -->
<scripts>
